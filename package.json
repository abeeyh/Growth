{
  "name": "vividus-beckend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "^6.59.2",
    "@tsed/common": "^6.59.2",
    "@tsed/core": "^6.59.2",
    "@tsed/di": "^6.59.2",
    "@tsed/exceptions": "^6.59.2",
    "@tsed/json-mapper": "^6.59.2",
    "@tsed/platform-express": "^6.59.2",
    "@tsed/schema": "^6.59.2",
    "@tsed/swagger": "^6.59.2",
    "@types/node-fetch": "^2.5.11",
    "ajv": "^8.6.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-tslint": "3.3.3",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.13",
    "@types/method-override": "0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  }
}
